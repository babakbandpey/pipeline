# OpenAI Configuration
OPENAI_API_KEY=your_openai_key_here  # Required for OpenAI API access

# Git Configuration
GIT_AUTHOR_NAME=Your Name            # Git commit author name
GIT_AUTHOR_EMAIL=your@email.com      # Git commit author email
GIT_SSH_KEY_WORK=~/.ssh/id_rsa_work      # Path to work SSH key
GIT_SSH_KEY_PERSONAL=~/.ssh/id_rsa_personal  # Path to personal SSH key
GIT_SSH_HOST_WORK=github.com-work        # SSH config host for work
GIT_SSH_HOST_PERSONAL=github.com-personal # SSH config host for personal

# Azure Configuration (Optional)
AZURE_OPENAI_ENDPOINT=your_azure_endpoint  # Azure OpenAI endpoint URL
AZURE_OPENAI_API_KEY=your_azure_key       # Azure OpenAI API key
